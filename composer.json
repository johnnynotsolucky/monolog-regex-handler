{
    "name": "johnnynotsolucky/monolog-regex-handler",
    "homepage": "https://github.com/johnnynotsolucky/monolog-regex-handler",
    "type": "library",
    "description": "Regex log filter handler for Monolog",
    "keywords": [
        "monolog"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tyrone Tudehope",
            "email": "hi@tyrone.dev",
            "homepage": "https://tyrone.dev"
        }
    ],
    "support": {
        "issues": "https://github.com/johnnynotsolucky/monolog-regex-handler/issues",
        "source": "https://github.com/johnnynotsolucky/monolog-regex-handler"
    },
    "require": {
        "php": "^7.2 || ^8",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "johnnynotsolucky\\RegexHandler\\": "src"
        }
    },
    "minimum-stability": "stable"
}
